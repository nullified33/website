.hl
  border solid 3px #808080
body
  background-color $lightblueish
  background-position 0 0 50px 50px
  background-size 100px 100px
  height 200px
  width 100%
  padding 10px
.ibox
  width 200px
  font-family IBM Plex Mono
  font-weight bold
  background-color #808080
  margin-left 20px
.topnav
  background-color $blueish
img
  border-style solid
  border-color #000
  border 5px
  border-radius 25px
  padding 8px
iframe
  border-style solid
  border-color #000
  border 0px
  border-radius 25px
  padding 5px
  width 450px
p
  font-family IBM Plex Mono
  font-weight bold
  font-kerning normal
  font-size 15px  
h1
  font-family IBM Plex Mono
  font-kerning normal
  font-weight bold
  font-size 45px
h2
  font-family IBM Plex Mono
  font-kerning normal
  font-weight bold
  font-size 30px
h3
  font-family IBM Plex Mono
  font-kerning normal
  font-weight normal
  font-size 25px
h4
  font-family IBM Plex Mono
  font-kerning normal
  font-weight normal
  font-size 20px  
h5
  font-family IBM Plex Mono
  font-kerning normal
  font-weight lighter
  font-style italic
  font-size 15px 
h6
  font-family IBM Plex Mono
  font-kerning normal
  font-weight lighter
  font-style italic
  font-size 10px 
h5.card-title
  font-family IBM Plex Mono
  font-kerning normal
  font-weight bold
  font-size 30px 
div
  font-family IBM Plex Mono
  font-weight bold
  font-kerning normal
button
  font-family Fira Code
  font-kerning normal
  background-color $blueish
  color #000
  border-radius 25px
  border 2px solid #000
  padding 12.5px
label
  border-left 5px solid #000
a:link
  color #fff
  background-color transparent
  text-decoration none
  font-family Fira Code !important
a:visited
  color #f5f5f5
  background-color transparent
  text-decoration none
  font-family Fira Code !important
  font-style italic
a:hover
  color #dcdcdc
  background-color transparent
  text-decoration underline
  font-family Fira Code !important
a:active
  color #1d1d01
  background-color transparent
  text-decoration underline
  font-family Fira Code !important
hr
  border-top 4px solid #000
li
  font-family IBM Plex Mono
  font-weight bold
  font-kerning normal
ol
  font-family IBM Plex Mono
  font-weight bold
  font-kerning normal
.dark-text
  color #808080 !important
.center-align
  text-align center
.round
  border-radius 25px !important
.center
  display block
  margin-left auto
  margin-right auto
.blueish
  background-color $blueish!important
.blutext
  color $blueish !important
  text-decoration none !important
.lightblueish
  background-color $lightblueish !important
pre code
  padding 50px
  background-color black
  border 3px solid $darkblueish
  color #fff !important
  display block
.fira-code
  font-family Fira Code !important
.ibm-plex-mono
  font-family IBM Plex Mono !important

/* Bootstrap 4.0 Stuff Holdovers */
.jumbotron
  padding 2rem 1rem
  margin-bottom 2rem
  background-color #e9ecef
  border-radius 0.3rem
@media (min-width 576px)
  .jumbotron
    padding 4rem 2rem
.jumbotron-fluid
  padding-right 0
  padding-left 0
  border-radius 0
/*Bootstrap Additions */
.mt-7 
  margin-top 12rem !important
/* End */
#footer-links .custom
  min-width 1em
#footer-links .custom img
  height 1em
  padding 0
kbd
  padding 0.4rem
  border-radius 1rem